<Controls:MetroWindow x:Class="EEPK_Organiser.Forms.EMP.EMP_Editor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ex="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:local="clr-namespace:EEPK_Organiser.Forms.EMP"
        xmlns:vector="clr-namespace:EEPK_Organiser.View.Vectors"
        xmlns:converters="clr-namespace:LB_Common.Converters;assembly=LB_Common"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        xmlns:undoRedo="clr-namespace:Xv2CoreLib.Resource.UndoRedo;assembly=Xv2CoreLib"
        xmlns:emp="clr-namespace:Xv2CoreLib.EMP;assembly=Xv2CoreLib"
        xmlns:empAnim="clr-namespace:Xv2CoreLib.EMP;assembly=Xv2CoreLib"
        xmlns:lbUtils="clr-namespace:LB_Common.Utils;assembly=LB_Common"
        xmlns:lbCommon="clr-namespace:LB_Common;assembly=LB_Common"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        x:Name="empEditorWindow" Title="EMP Editor" Height="575" Width="1110" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen" AllowDrop="True" WindowStyle="ThreeDBorderWindow" TitleCharacterCasing="Normal">
    <Window.InputBindings>
        <KeyBinding Key="Z" Modifiers="Ctrl"  Command="{Binding Source={x:Static undoRedo:UndoManager.Instance}, Path=UndoCommand}"/>
        <KeyBinding Key="Y" Modifiers="Ctrl"  Command="{Binding Source={x:Static undoRedo:UndoManager.Instance}, Path=RedoCommand}"/>
    </Window.InputBindings>
    <Window.Resources>
        <converters:ColorFormatConverter x:Key="RgbConverter" />
        <converters:NullToFalseConverter x:Key="NullToFalse" />

        <ObjectDataProvider x:Key="AnimTypeEnum" MethodName="GetValues" ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="emp:Type0+Parameter"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
    </Window.Resources>
    <Grid x:Name="rootGrid">

        <TabControl x:Name="mainTabControl" HorizontalAlignment="Left" Height="537" VerticalAlignment="Top" Width="1095" Margin="0, 2">
            <Controls:MetroTabItem Header="Particle Effects" Controls:HeaderedControlHelper.HeaderFontSize="16">
                <Grid Margin="0,0,-6.836,-0.055">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="40"/>
                        <ColumnDefinition Width="290"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TreeView x:Name="empTree" Grid.Column="1" ItemsSource="{Binding empFile.ParticleEffects.Binding}" BorderBrush="{DynamicResource MahApps.Brushes.Accent2}" BorderThickness="1" SelectedItemChanged="empTree_SelectedItemChanged" Margin="3, 5">
                        <TreeView.InputBindings>
                            <KeyBinding Key="Delete" Command="{Binding RemoveParticleEffect_Command}"/>
                            <KeyBinding Key="N" Modifiers="Ctrl" Command="{Binding AddParticleEffect_Command}"/>
                            <KeyBinding Key="C" Modifiers="Ctrl" Command="{Binding Copy_Command}"/>
                            <KeyBinding Key="V" Modifiers="Ctrl" Command="{Binding Paste_Command}"/>
                            <KeyBinding Key="V" Modifiers="Ctrl+Alt" Command="{Binding PasteChild_Command}"/>
                            <KeyBinding Key="X" Modifiers="Ctrl" Command="{Binding PasteValues_Command}"/>
                            <KeyBinding Key="Up" Modifiers="Ctrl" Command="{Binding MoveUp_Command}"/>
                            <KeyBinding Key="Down" Modifiers="Ctrl" Command="{Binding MoveDown_Command}"/>
                            <KeyBinding Key="H" Modifiers="Ctrl" Command="{Binding HueAdjustment_Command}"/>
                        </TreeView.InputBindings>
                        <TreeView.Resources>
                            <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="{DynamicResource {x:Static SystemColors.HighlightColorKey}}" />
                            <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightTextBrushKey}" Color="{DynamicResource {x:Static SystemColors.HighlightTextColorKey}}"/>
                            <HierarchicalDataTemplate DataType="{x:Type emp:ParticleEffect}" ItemsSource="{Binding ChildParticleEffects.Binding}">
                                <StackPanel Tag="{Binding DataContext, ElementName=empEditorWindow}">
                                    <TextBlock Text="{Binding Name, UpdateSourceTrigger=PropertyChanged}" MinWidth="100" />
                                    <StackPanel.ContextMenu>
                                        <ContextMenu StaysOpen="true" FontSize="12">
                                            <MenuItem Header="Add New Above" Command="{Binding PlacementTarget.Tag.AddNewAbove_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Add New Below" Command="{Binding PlacementTarget.Tag.AddNewBelow_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Add New As Child" Command="{Binding PlacementTarget.Tag.AddParticleEffectAsChild_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Remove" Command="{Binding PlacementTarget.Tag.RemoveParticleEffect_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <Separator/>
                                            <MenuItem Header="Move Up" Command="{Binding PlacementTarget.Tag.MoveUp_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Move Down" Command="{Binding PlacementTarget.Tag.MoveDown_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <Separator/>
                                            <MenuItem Header="Copy" Command="{Binding PlacementTarget.Tag.Copy_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Paste" Command="{Binding PlacementTarget.Tag.Paste_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Paste As Child" Command="{Binding PlacementTarget.Tag.PasteChild_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Paste Values" Command="{Binding PlacementTarget.Tag.PasteValues_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <Separator/>
                                            <MenuItem Header="Hue Adjustment" Command="{Binding PlacementTarget.Tag.HueAdjustment_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Hue Set" Command="{Binding PlacementTarget.Tag.HueSet_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <Separator/>
                                            <MenuItem Header="Hide All" Command="{Binding PlacementTarget.Tag.HideAll_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Show All" Command="{Binding PlacementTarget.Tag.ShowAll_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Hide All Selected" Command="{Binding PlacementTarget.Tag.HideAllSelected_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                            <MenuItem Header="Show All Selected" Command="{Binding PlacementTarget.Tag.ShowAllSelected_Command, RelativeSource={RelativeSource AncestorType=ContextMenu}}"/>
                                        </ContextMenu>
                                    </StackPanel.ContextMenu>
                                </StackPanel>
                            </HierarchicalDataTemplate>

                        </TreeView.Resources>
                        </TreeView>

                    <TabControl x:Name="tabControl_ParticleEffect" Grid.Column="2" BorderBrush="{DynamicResource MahApps.Brushes.Accent2}" BorderThickness="1" Margin="5, 5">
                        <Controls:MetroTabItem Header="General" Controls:HeaderedControlHelper.HeaderFontSize="16">
                            <Grid Margin="0,0,0.055,0">
                                <TextBox x:Name="general_Name" Text="{Binding SelectedItem.Name, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="134,57,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="253"/>
                                <Label Content="Name" HorizontalAlignment="Left" Margin="30,55,0,0" VerticalAlignment="Top" Width="104" HorizontalContentAlignment="Right" ToolTip="The name of the Particle Effect. Has a maximum allowed length of 32 characters."/>
                                <TextBox x:Name="general_ParticleLifetime" Text="{Binding SelectedItem.I_40, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="134,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Particle Lifetime" HorizontalAlignment="Left" Margin="0,115,0,0" VerticalAlignment="Top" Width="134" HorizontalContentAlignment="Right" ToolTip="The number of frames that this particle will last."/>
                                <TextBox x:Name="general_ParticleLifetimeAddRandom" Text="{Binding SelectedItem.I_42, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="305,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Variance" HorizontalAlignment="Left" Margin="224,115,0,0" VerticalAlignment="Top" Width="81" HorizontalContentAlignment="Right"/>
                                <TextBox x:Name="general_ParticleCount" Text="{Binding SelectedItem.I_38, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="134,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Particle Count" HorizontalAlignment="Left" Margin="0,146,0,0" VerticalAlignment="Top" Width="134" HorizontalContentAlignment="Right" ToolTip="The maximum possible instances of this particle that can be active at the same time. For use with particle effects that are children to a spawner type - does nothing on the root entry."/>
                                <TextBox x:Name="general_NewDelay" Text="{Binding SelectedItem.I_46, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="134,180,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Create Delay" HorizontalAlignment="Left" Margin="0,177,0,0" VerticalAlignment="Top" Width="134" HorizontalContentAlignment="Right" ToolTip="The number of frames that must pass before new particles are created. For use only on spawner-type particle effects."/>
                                <TextBox x:Name="general_NewDelayAddRandom" Text="{Binding SelectedItem.I_47, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="305,180,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Variance" HorizontalAlignment="Left" Margin="224,177,0,0" VerticalAlignment="Top" Width="81" HorizontalContentAlignment="Right"/>
                                <TextBox x:Name="general_MaxParticlesPerFrame" Text="{Binding SelectedItem.I_52, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="134,211,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Max Particles (frame)" HorizontalAlignment="Left" Margin="0,207,0,0" VerticalAlignment="Top" Width="134" HorizontalContentAlignment="Right" ToolTip="The maximum amount of particles that can be created from this entry in a single frame. For use with spawner-type particle effects."/>
                                <TextBox x:Name="general_MaxParticlesPerFrameAddRandom" Text="{Binding SelectedItem.I_54, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="305,211,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Variance" HorizontalAlignment="Left" Margin="224,207,0,0" VerticalAlignment="Top" Width="81" HorizontalContentAlignment="Right"/>
                                <GroupBox Header="Position" HorizontalAlignment="Left" Height="141" Margin="479,25,0,0" VerticalAlignment="Top" Width="241">
                                    <Grid>
                                        <TextBox x:Name="general_Transform_X" Text="{Binding SelectedItem.F_64, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="32,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="X" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="22" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_X_AddRandom" Text="{Binding SelectedItem.F_80, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="160,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Variance" HorizontalAlignment="Left" Margin="96,10,0,0" VerticalAlignment="Top" Width="63" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_Y" Text="{Binding SelectedItem.F_68, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="32,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Y" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Width="22" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_Y_AddRandom" Text="{Binding SelectedItem.F_84, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="160,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Variance" HorizontalAlignment="Left" Margin="96,40,0,0" VerticalAlignment="Top" Width="63" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_Z" Text="{Binding SelectedItem.F_72, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="32,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Z" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="22" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_Z_AddRandom" Text="{Binding SelectedItem.F_88, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="160,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Variance" HorizontalAlignment="Left" Margin="96,70,0,0" VerticalAlignment="Top" Width="63" HorizontalContentAlignment="Right"/>

                                    </Grid>
                                </GroupBox>
                                <GroupBox Header="Rotation" HorizontalAlignment="Left" Height="143" Margin="479,177,0,0" VerticalAlignment="Top" Width="241">
                                    <Grid>
                                        <TextBox x:Name="general_Transform_X1" Text="{Binding SelectedItem.F_96, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="32,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="X" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="22" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_X_AddRandom1" Text="{Binding SelectedItem.F_112, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="166,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Variance" HorizontalAlignment="Left" Margin="99,10,0,0" VerticalAlignment="Top" Width="66" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_Y1" Text="{Binding SelectedItem.F_100, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="32,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Y" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Width="22" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_Y_AddRandom1" Text="{Binding SelectedItem.F_116, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="166,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Variance" HorizontalAlignment="Left" Margin="99,40,0,0" VerticalAlignment="Top" Width="66" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_Z1" Text="{Binding SelectedItem.F_104, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="32,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Z" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="22" HorizontalContentAlignment="Right"/>
                                        <TextBox x:Name="general_Transform_Z_AddRandom1" Text="{Binding SelectedItem.F_120, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="166,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
                                        <Label Content="Variance" HorizontalAlignment="Left" Margin="99,70,0,0" VerticalAlignment="Top" Width="66" HorizontalContentAlignment="Right"/>

                                    </Grid>
                                </GroupBox>
                                <CheckBox x:Name="checkBox_General_Loop" IsChecked="{Binding SelectedItem.I_32_1, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" Content="Loop" HorizontalAlignment="Left" Margin="15,270,0,0" VerticalAlignment="Top" Width="171"/>
                                <CheckBox x:Name="checkBox_General_FlashOnGen" IsChecked="{Binding SelectedItem.I_32_3, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" Content="Flash on generation" HorizontalAlignment="Left" Margin="15,291,0,0" VerticalAlignment="Top" Width="171"/>
                                <CheckBox x:Name="checkBox_General_UseScale2" IsChecked="{Binding SelectedItem.I_33_4, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" Content="Use Scale2" HorizontalAlignment="Left" Margin="15,312,0,0" VerticalAlignment="Top" Width="171"/>
                                <CheckBox x:Name="checkBox_General_UseColor2" IsChecked="{Binding SelectedItem.I_33_6, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" Content="Use Color2" HorizontalAlignment="Left" Margin="15,333,0,0" VerticalAlignment="Top" Width="171"/>
                                <ComboBox SelectedItem="{Binding SelectedItem.Component_Type, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" ItemsSource="{Binding SelectedItem.ComponentTypeValues, ElementName=empTree}" HorizontalAlignment="Left" Margin="134,27,0,0" VerticalAlignment="Top" Width="253" SelectionChanged="ComponentType_SelectionChanged"/>
                                <Label Content="Component Type" HorizontalAlignment="Left" Margin="10,27,0,0" VerticalAlignment="Top" Width="124" HorizontalContentAlignment="Right"/>
                                <ComboBox SelectedItem="{Binding SelectedItem.I_35, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" ItemsSource="{Binding SelectedItem.AutoOrientationTypes, ElementName=empTree}" HorizontalAlignment="Left" Margin="134,240,0,0" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Auto Orientation Type" HorizontalAlignment="Left" Margin="0,237,0,0" VerticalAlignment="Top" Width="134" HorizontalContentAlignment="Right"/>
                                <CheckBox x:Name="checkBox_General_Hide" IsChecked="{Binding SelectedItem.I_33_3, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" Content="Hide" HorizontalAlignment="Left" Margin="160,270,0,0" VerticalAlignment="Top" Width="171"/>
                                <TextBox x:Name="general_StartTime" Text="{Binding SelectedItem.I_44, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="134,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Start Time" HorizontalAlignment="Left" Margin="0,85,0,0" VerticalAlignment="Top" Width="134" HorizontalContentAlignment="Right" ToolTip="Delays the start of this particle by the specified amount of frames. Does not shorten the life time."/>
                                <TextBox x:Name="general_StartTimeAddRandom" Text="{Binding SelectedItem.I_45, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="305,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="Variance" HorizontalAlignment="Left" Margin="224,85,0,0" VerticalAlignment="Top" Width="81" HorizontalContentAlignment="Right"/>
                                <CheckBox x:Name="checkBox_General_EnableRandomRotationDirection" IsChecked="{Binding SelectedItem.I_34_4, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" Content="EnableRandomRotationDirection" HorizontalAlignment="Left" Margin="160,291,0,0" VerticalAlignment="Top" Width="284" ToolTip="Enables random behaviour for SpeedRotation/AngleRotation values.&#x0a;&#x0a;When enabled, a value of 50 has an equal chance of being either 50 or -50."/>
                                <CheckBox x:Name="checkBox_General_EnableRandomUpVectorOnVirtualCone" IsChecked="{Binding SelectedItem.I_34_6, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" Content="EnableRandomUpVectorOnVirtualCone" HorizontalAlignment="Left" Margin="160,312,0,0" VerticalAlignment="Top" Width="284"/>
                                <TextBox x:Name="general_F_128" Text="{Binding SelectedItem.F_128, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="79,364,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="F__128" HorizontalAlignment="Left" Margin="10,363,0,0" VerticalAlignment="Top" Width="69" HorizontalContentAlignment="Right"/>
                                <TextBox x:Name="general_F_132" Text="{Binding SelectedItem.F_132, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="235,363,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="F__132" HorizontalAlignment="Left" Margin="176,362,0,0" VerticalAlignment="Top" Width="59" HorizontalContentAlignment="Right"/>
                                <TextBox x:Name="general_I_136" Text="{Binding SelectedItem.I_136, ElementName=empTree, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="377,363,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
                                <Label Content="I__136" HorizontalAlignment="Left" Margin="322,362,0,0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Right"/>
                            </Grid>

                        </Controls:MetroTabItem>
                        <Controls:MetroTabItem x:Name="tab_TexturePart" Header="Texture" Controls:HeaderedControlHelper.HeaderFontSize="16">
                            <TabControl>
                                <TabItem Header="Main" Controls:HeaderedControlHelper.HeaderFontSize="16">
                                    <Grid x:Name="grid_TexturePart" Margin="0,0,-0.055,0">

                                        <StackPanel Margin="10, 10" Width="456" HorizontalAlignment="Left">

                                            <TextBlock Text="Texture" FontWeight="Bold" HorizontalAlignment="Center"/>
                                            <Grid Margin="3,3" Width="450" HorizontalAlignment="Left">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="120"/>
                                                    <ColumnDefinition Width="217"/>
                                                    <ColumnDefinition Width="30"/>
                                                    <ColumnDefinition Width="30"/>
                                                </Grid.ColumnDefinitions>
                                                <Label Content="Material"/>
                                                <ComboBox Grid.Column="1" DisplayMemberPath="Name" SelectedItem="{Binding SelectedItem.Type_Texture.MaterialRef, ElementName=empTree}" ItemsSource="{Binding materialFile.Materials}" Controls:TextBoxHelper.Watermark="No Material Selected"/>

                                                <Button Grid.Column="2" Command="{Binding ElementName=empEditorWindow, Path=TexturePart_RemoveMaterialCommand}" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{DynamicResource MahApps.Styles.Button.Circle}" Width="20" Height="20" ToolTip="Remove the selected material from this particle effect.">
                                                    <iconPacks:PackIconMaterialDesign Kind="RemoveCircleOutline"/>
                                                </Button>
                                                <Button Grid.Column="3" Command="{Binding ElementName=empEditorWindow, Path=TexturePart_GotoMaterialCommand}" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{DynamicResource MahApps.Styles.Button.Circle}" Width="20" Height="20" ToolTip="Jump to the selected material, opening the Materials Editor.">
                                                    <iconPacks:PackIconMaterialDesign Kind="KeyboardArrowRight"/>
                                                </Button>
                                            </Grid>

                                            <Grid Margin="3,3" Width="450" HorizontalAlignment="Left">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="120"/>
                                                    <ColumnDefinition Width="217"/>
                                                    <ColumnDefinition Width="30"/>
                                                    <ColumnDefinition Width="30"/>
                                                </Grid.ColumnDefinitions>
                                                <Label Content="Texture"/>
                                                <ComboBox Grid.Column="1" DisplayMemberPath="ToolName" SelectedItem="{Binding SelectedItem.Type_Texture.TextureEntryRef[0].UndoableTextureRef, ElementName=empTree}" ItemsSource="{Binding empFile.Textures}" Controls:TextBoxHelper.Watermark="No Texture Selected"/>
                                                <Button Grid.Column="2" Command="{Binding ElementName=empEditorWindow, Path=TexturePart_RemoveTextureCommand}" CommandParameter="0" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{DynamicResource MahApps.Styles.Button.Circle}" Width="20" Height="20" ToolTip="Remove the selected texture from this particle effect.">
                                                    <iconPacks:PackIconMaterialDesign Kind="RemoveCircleOutline"/>
                                                </Button>
                                                <Button Grid.Column="3" Command="{Binding ElementName=empEditorWindow, Path=TexturePart_GotoTextureCommand}" CommandParameter="0" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{DynamicResource MahApps.Styles.Button.Circle}" Width="20" Height="20" ToolTip="Jump to the selected texture.">
                                                    <iconPacks:PackIconMaterialDesign Kind="KeyboardArrowRight"/>
                                                </Button>
                                            </Grid>
                                            <Grid Margin="3,3" Width="450" HorizontalAlignment="Left">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="120"/>
                                                    <ColumnDefinition Width="217"/>
                                                    <ColumnDefinition Width="30"/>
                                                    <ColumnDefinition Width="30"/>
                                                </Grid.ColumnDefinitions>
                                                <Label Content="Texture (Secondary)" ToolTip="A secondary texture that particle effects can use in some cases."/>
                                                <ComboBox Grid.Column="1" DisplayMemberPath="ToolName" SelectedItem="{Binding SelectedItem.Type_Texture.TextureEntryRef[1].UndoableTextureRef, ElementName=empTree}" ItemsSource="{Binding empFile.Textures}" Controls:TextBoxHelper.Watermark="No Texture Selected"/>
                                                <Button Grid.Column="2" Command="{Binding ElementName=empEditorWindow, Path=TexturePart_RemoveTextureCommand}" CommandParameter="1" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{DynamicResource MahApps.Styles.Button.Circle}" Width="20" Height="20" ToolTip="Remove the selected texture from this particle effect.">
                                                    <iconPacks:PackIconMaterialDesign Kind="RemoveCircleOutline"/>
                                                </Button>
                                                <Button Grid.Column="3" Command="{Binding ElementName=empEditorWindow, Path=TexturePart_GotoTextureCommand}" CommandParameter="1" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{DynamicResource MahApps.Styles.Button.Circle}" Width="20" Height="20" ToolTip="Jump to the selected texture.">
                                                    <iconPacks:PackIconMaterialDesign Kind="KeyboardArrowRight"/>
                                                </Button>
                                            </Grid>


                                            <TextBlock Text="Color" FontWeight="Bold" HorizontalAlignment="Center"/>
                                            <Grid Margin="3,3" Width="450" HorizontalAlignment="Left">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="100"/>
                                                    <ColumnDefinition/>
                                                </Grid.ColumnDefinitions>
                                                <Label Content="Color1"/>
                                                <vector:CustomColor Grid.Column="1" Value="{Binding SelectedItem.Type_Texture.Color1, ElementName=empTree}"/>
                                            </Grid>
                                            <Grid Margin="3,3" Width="450" HorizontalAlignment="Left">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="100"/>
                                                    <ColumnDefinition/>
                                                </Grid.ColumnDefinitions>
                                                <Label Content="Color2"/>
                                                <vector:CustomColor Grid.Column="1" Value="{Binding SelectedItem.Type_Texture.Color2, ElementName=empTree}"/>
                                            </Grid>
                                            <Grid Margin="3,3" Width="450" HorizontalAlignment="Left">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="100"/>
                                                    <ColumnDefinition/>
                                                </Grid.ColumnDefinitions>
                                                <Label Content="Color Variance"/>
                                                <vector:CustomColor Grid.Column="1" Value="{Binding SelectedItem.Type_Texture.Color_Variance, ElementName=empTree}"/>
                                            </Grid>
                                            <TextBlock Text="Scale" FontWeight="Bold" HorizontalAlignment="Center"/>
                                            <Grid Margin="3,3" Width="450" HorizontalAlignment="Left">
